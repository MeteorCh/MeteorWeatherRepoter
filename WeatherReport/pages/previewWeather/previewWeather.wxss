@import "../template/template.wxss";

.previewTitle{
  height: 90rpx;
  color: gray;
  font-size: 40rpx;
  width: 100%;
  text-align: center;
  margin: 20rpx 0;
}

.previewContent{
  display: flex;
  flex-direction: column;
}

.footer{
  height: 150rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items:center;
}

.footer .iconContainer{
  height: 80rpx;
  width: 80rpx;
  border-radius: 40rpx;
  background: lightgray;
  display:flex;
  align-items:center;
  justify-content:center;
}

.footer .iconContainer .pic{
  width: 50rpx;
  height: 50rpx;
}

.footer .des{
  color: gray;
  margin-top: 20rpx;
  font-size: 20rpx;
  margin-bottom: 10rpx;
}
